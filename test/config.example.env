# WHOOP SDK Test Configuration
# Copy this file to .env in the root directory and fill in your values

# WHOOP OAuth Application Credentials
# Get these from https://developer.whoop.com
WHOOP_CLIENT_ID=your_client_id_here
WHOOP_CLIENT_SECRET=your_client_secret_here

# OAuth Redirect URI
# This must match the redirect URI configured in your WHOOP app
# Common options:
WHOOP_REDIRECT_URI=http://localhost:8080/api/whoop/callback
# WHOOP_REDIRECT_URI=http://localhost:8080
# WHOOP_REDIRECT_URI=http://127.0.0.1:8080/api/whoop/callback

# Server Port (for OAuth callback server)
PORT=8080

# Replit Configuration (if running on Replit)
# REPLIT_DEV_DOMAIN=your-replit-domain.repl.co

# Optional: Base URL override (usually not needed)
# WHOOP_BASE_URL=https://api.prod.whoop.com/developer

# Performance Configuration (optional)
# These settings can also be configured programmatically
WHOOP_CACHE_TTL=300000
WHOOP_CACHE_MAX_SIZE=1000
WHOOP_CACHE_COMPRESSION=true
WHOOP_DEDUP_ENABLED=true
WHOOP_DEDUP_WINDOW_MS=100
WHOOP_MAX_CONCURRENT=10
WHOOP_SLOW_REQUEST_THRESHOLD=2000 